{{> nav/nav-bar home-active=true}}

<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">Fluid jumbotron</h1>
    <p class="lead">Home Page</p>
  </div>
</div>


{{#each articles}}
	<div class="card">
	  <h5 class="card-header">{{ this.headline }}</h5>
	  <div class="card-body">
	    <p class="card-text">{{ this.summary }}</p>
	    <a href="#" class="btn btn-primary">Go somewhere</a>
	  </div>
	</div>
{{/each}}